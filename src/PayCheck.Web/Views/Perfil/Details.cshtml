@model ARVTech.DataAccess.DTOs.UniPayCheck.PessoaFisicaResponse

@{
    ViewData["Title"] = "Details";

    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!-- Begin Page Content -->
<div class="container-fluid">
    <!-- Page Heading -->
    <div class="d-sm-flex align-items-center justify-content-between mb-4">
        <h1 class="h3 mb-0 text-gray-800">Demonstrativo de Pagamento</h1>
        <a href="#" class="d-none d-sm-inline-block btn btn-sm btn-primary shadow-sm">
            <i class="fas fa-print fa-sm text-white-50"></i> Modelo Clássico
        </a>
    </div>

    <div class="row">
        <div class="col-xl-7 col-md-7 mb-4">
            <label for="txtRazaoSocial">Razão Social</label>
            <input asp-for=@Model.Nome type="text" class="form-control" id="txtNome">
        </div>

        <div class="col-xl-3 col-md-3 mb-4">
            <label for="txtCpf">CPF</label>
            <input asp-for=@Model.CpfFormatado type="text" class="form-control" id="txtCpf">
        </div>

        <div class="col-xl-2 col-md-2 mb-4">
            <label for="txtDataNascimento">Data Nascimento</label>
            <input asp-for=@Model.DataNascimentoFormatada type="text" class="form-control" id="txtDataNascimento">
        </div>
    </div>

    <div class="row mb-3">
        
    </div>

    <div>
        @*@Html.ActionLink("Edit", "Edit", new { /* id = Model.PrimaryKey */ }) |*@
        <a asp-action="Index">Voltar</a>
    </div>
</div>